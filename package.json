{
  "name": "carexpenses-api",
  "version": "1.0.12",
  "description": "A multi-tenant SaaS platform for tracking car expenses and trips",
  "main": "build/index.js",
  "scripts": {
    "cmd": "ts-node -r tsconfig-paths/register --project ./tsconfig.json ./src/app/cli/index.ts ",
    "build": "cross-env NODE_ENV=production tsc -p ./tsconfig.json",
    "start": "cpy .env.example dist && node dist/src/index.js",
    "start:dev": "nodemon --exec \"ts-node -r tsconfig-paths/register --project ./tsconfig.json ./src/index.ts\" -e ts",
    "lint": "eslint */**/*.{js,ts,tsx} --quiet --fix",
    "test": "jest --config jest.config.ts",
    "create-migration": "knex migrate:make -x ts --knexfile ./src/knexConfig.ts",
    "migrations": "knex migrate:latest --knexfile ./src/knexConfig.ts",
    "migration-down": "knex migrate:down --knexfile ./src/knexConfig.ts",
    "rollback-migration": "knex migrate:rollback --knexfile ./src/knexConfig.ts",
    "seed": "knex seed:run --knexfile ./src/knexConfig.ts",
    "reset-db": "node cli -r",
    "build-schema": "rover graph introspect http://localhost:4060/graphql > microservice-storage.graphql",
    "publish-schema": "env-cmd rover subgraph publish ShopaDropaGraph@current --name microservice-storage --schema ./microservice-storage.graphql --routing-url http://10.12.12.249/graphql"
  },
  "author": "Sergey Demidenko",
  "license": "ISC",
  "dependencies": {
    "@apollo/server": "^5.2.0",
    "@apollo/subgraph": "^2.12.2",
    "@as-integrations/express5": "^1.1.2",
    "@azure/web-pubsub": "^1.2.0",
    "@graphql-tools/load-files": "^7.0.1",
    "@graphql-tools/merge": "^9.1.7",
    "@graphql-tools/schema": "^10.0.31",
    "@graphql-tools/utils": "^11.0.0",
    "@microsoft/signalr": "^9.0.6",
    "@sdflc/api-helpers": "^1.1.13",
    "@sdflc/backend-helpers": "^1.0.112",
    "@sdflc/utils": "^1.0.25",
    "@sendgrid/mail": "^8.1.6",
    "@types/mime-types": "^3.0.1",
    "axios": "^1.13.2",
    "bcrypt": "^6.0.0",
    "checkit": "^0.7.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "cpy-cli": "^6.0.0",
    "cross-env": "^10.1.0",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.19",
    "dotenv-safe": "^9.1.0",
    "env-cmd": "^11.0.0",
    "express": "^5.2.1",
    "file-type": "^21.3.0",
    "is-uuid": "^1.0.2",
    "jest": "^30.1.3",
    "jsonwebtoken": "^9.0.3",
    "knex": "^3.1.0",
    "lodash": "^4.17.21",
    "mime-types": "^3.0.2",
    "mssql": "^11.0.1",
    "multer": "^2.0.2",
    "nodemailer": "^7.0.12",
    "raw-body": "^3.0.2",
    "redis": "^5.10.0",
    "rimraf": "^6.1.2",
    "stripe": "^18.5.0",
    "typescript": "^5.9.3",
    "uuid": "^11.1.0",
    "yargs": "^18.0.0"
  },
  "devDependencies": {
    "@apollo/rover": "^0.35.0",
    "@types/adm-zip": "^0.5.7",
    "@types/body-parser": "^1.19.6",
    "@types/cors": "^2.8.19",
    "@types/dotenv-safe": "^8.1.6",
    "@types/express": "^5.0.3",
    "@types/lodash": "^4.17.20",
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^7.0.4",
    "@types/stripe": "^8.0.417",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.4",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "ts-jest": "^29.4.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  }
}
